---
title: Donor and Member Wall
---

<div class="splash">
  <div class="splash-wrapper">
    <div class="splash-greeting">
      <h1 class="splash-header">Donor and Member Wall</h1>
      <p class="splash-info"><%= data.Info[0]['about_trib'] %></p>
      <h4 class="splash-support"><%= data.Info[0]['support_cta'] %></h4>
    </div><!-- splash-greeting -->
  </div><!-- splash-wrapper -->
  <%= partial "desktop_nav" %>
</div><!-- splash-levels -->

<div class="donor-wall-wrapper">
	<div class="search-container">
		<span class="search-label">Search <span class="search-year"></span> donors: </span><input id="donor-search" />
		<button class="button button-flat" id="go">Go</button>
		<button class="button button-flat" id="reset">Reset</button>
	</div>
	<select id="year-select">
		<option class="select-year all-time">All time</option>
	</select>
	<h3 class="sponsor-table-header"><span id="search-year"></span> Donors</h3>
	<div class="donor-wall-table"></div>
</div>